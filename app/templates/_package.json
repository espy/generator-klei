  "scripts": {<% if (express) { %>
    "start": "node ."
  <% } %>},
  <% if (express) { %>"main": "index.js",
  <% } %>"keywords": [
    "klei-module"
  ],
  "dependencies": {
    <% if (mongo) { %>"mongoose": "~3.8.0",
    "mongoload": "~0.1.0"<% } %><% if (mongo && express) { %>,
