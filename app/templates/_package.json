    "exctrl": ">=0.2"<% } %>
  },
  "devDependencies": {
    "load-grunt-tasks": "~0.2.0",
    "grunt": "~0.4.1",
    "grunt-concurrent": "~0.4.1",
    "grunt-contrib-copy": "~0.4.1",
    "grunt-contrib-watch": "~0.5.3",
    "grunt-contrib-jshint": "~0.7.1",
    "grunt-newer": "~0.6.0",
    "grunt-express": "~1.2.1",
    "mocha": "~1.14.0",
    "jshint-stylish": "~0.1.3"<% if (angular || stylus) { %>,
